{
  "name": "nx-admin-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && npx husky install",
    "start": "nx serve",
    "start:web-client": "nx serve web-client",
    "start:api": "nx serve api",
    "build": "nx build",
    "test": "nx run-many --all --target=test",
    "lint": "nx run-many --all --target=lint",
    "lint:style": "stylelint {apps,libs}/**/*.{css,scss}",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name",
    "prisma:studio": "npx prisma studio",
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:seed": "prisma db seed",
    "docker:compose:up": "docker-compose up -d",
    "sudo:docker:compose:up": "sudo docker-compose up -d",
    "docker:compose:down": "docker-compose down --remove-orphans --volumes",
    "sudo:docker:compose:down": "sudo docker-compose down --remove-orphans --volumes"
  },
  "private": true,
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix",
    "*": "prettier -w -u"
  },
